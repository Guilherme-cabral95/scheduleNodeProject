<br/>
<button  type="button" class="btn btn-info" >
    <a href="/new_contacts" >
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" width="20" height="20"><path fill-rule="evenodd" d="M13.25 2.5H2.75a.25.25 0 00-.25.25v10.5c0 .138.112.25.25.25h10.5a.25.25 0 00.25-.25V2.75a.25.25 0 00-.25-.25zM2.75 1h10.5c.966 0 1.75.784 1.75 1.75v10.5A1.75 1.75 0 0113.25 15H2.75A1.75 1.75 0 011 13.25V2.75C1 1.784 1.784 1 2.75 1zM8 4a.75.75 0 01.75.75v2.5h2.5a.75.75 0 010 1.5h-2.5v2.5a.75.75 0 01-1.5 0v-2.5h-2.5a.75.75 0 010-1.5h2.5v-2.5A.75.75 0 018 4z"></path></svg>
    </a>
</button>
<table class="table table-contats">
    <thead class="table-light">
        <tr>
            <th>Nome</th>
            <th>E - mail</th>
            <th>Numero</th>
            <th>Ação</th>
        </tr>
    </thead>
    <tbody>
        <% contacts.forEach(res=>{%>
            <tr>
                <td><%= res.name %> </td>
                <td><%= res.email %></td>
                <td><%= res.phone %></td>
                <td>

                    <%- include('../templates/buttonDeleted',{id:res._id})  %> 
                    <%- include('../templates/buttonEdit',{id:res._id})  %> 
                </td>
            </tr>

        <%}) %> 
    </tbody>
  </table>